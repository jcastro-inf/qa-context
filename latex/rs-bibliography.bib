    journal                    = {{JASSS-THE JOURNAL OF ARTIFICIAL SOCIETIES AND SOCIAL SIMULATION}},
    keywords                   = {{Recommendation Strategy; Simulation Study; Way-Finding; Collaborative Filtering; Rating}},
    language                   = {{English}},
    location                   = {{UNIV SURREY, DEPT SOCIOLOGY, GUILDFORD GU2 7XH, SURREY, ENGLAND}},
    month                      = {{JAN}},
    number                     = {{1}},
    number-of-cited-references = {{43}},
