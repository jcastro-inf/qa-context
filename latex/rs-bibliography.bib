    journal                    = {{User Modelling and User-Adapted Interaction}},
    keywords                   = {{recommender systems; information filtering; user modeling; relevance feedback; Selective Dissemination of Information; machine learning; adaptive information retrieval}},
    keywords-plus              = {{PROBABILITY RANKING PRINCIPLE; INFORMATION-RETRIEVAL; RELEVANCE}},
    language                   = {{English}},
    location                   = {{SPUIBOULEVARD 50, PO BOX 17, 3300 AA DORDRECHT, NETHERLANDS}},
