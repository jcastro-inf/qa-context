    issn         = {1573-7721},
    journal      = {Multimedia Tools and Applications},
    pages        = {1--16},
    title        = {An effective web page recommender system with fuzzy c-mean clustering},
    url          = {http://dx.doi.org/10.1007/s11042-016-4078-7},
    year         = {2016},
}

