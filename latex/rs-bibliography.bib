@article{Castro2014,
    abstract     = {Content-based recommender systems (CBRS) are tools that help users to choose items when they face a huge amount of options, recommending items that better fit the user's profile. In such a process, it is very interesting to know which features of the items are more important for each user, thus the CBRS provides them higher weight. The Term Frequency-Inverse Document Frequency (TF-IDF) method is one of the most used for weighting of features, however, it does not provide the best results when the features are multi-valued. In this contribution, it is proposed a new method for obtaining the weights of the features by means of entropy and coefficients of dependency.},
    author       = {Castro, Jorge and Rodr\'iguez, Rosa M. and Barranco, Manuel J.},
    issn         = {1875-6891},
    journal      = {International Journal of Computational Intelligence Systems},
    number       = {1},
    pages        = {80-89},
    title        = {Weighting of Features in Content-based Filtering with Entropy and Dependence Measures},
    volume       = {7},
    year         = {2014},
}

@inproceedings{Castro2014b,
    abstract  = {Los sistemas de recomendaci\'on a grupos se utilizan para realizar recomendaciones a grupos de usuarios de forma que satisfagan sus necesidades como grupo de entre un conjunto sobrecargado de alternativas. El an\'alisis de las necesidades del grupo puede enfocarse de distintas formas atendiendo a la formaci\'on y relaci\'on del mismo. Una posibilidad es usar una representaci\'on del mismo, como si de una red social se tratase, mediante grafos difusos y as\'i aplicar el Paradigma de An\'alisis Inteligente de Redes Sociales (Paradigm of Intelligent Social Network Analysis, PISNA) para analizar dichas necesidades. En esta contribuci\'on se presenta una herramienta software S-PISNA que implementa distintas funciones de PISNA basada en computaci\'on con palabras, que nos ayudar\'a a mejorar procesos futuros de la recomendaci\'on a grupos.},
    author    = {Jorge Castro Gallardo and Salud Mar\'ia Jim\'enez Zafra and Manuel J. Barranco Garc\'ia and Luis Mart\'inez L\'opez},
    booktitle = {ESTYLF 2014 - XVII Congreso espa{\~n}ol sobre tecnolog\'ias y l\'ogica fuzzy},
    pages     = {175--180},
    title     = {S-PISNA: Una herramienta de an\'alisis para grafos difusos basada en computaci\'on con palabras para la mejora de recomendaci\'on a grupos},
    year      = {2014},
}

@inproceedings{Castro2014c,
    abstract     = {New trends in recommender systems face new challenges as group recommendation, in which users give their preferences over items and the system provides recommendations for a group of known users. In certain types of groups, it often occurs that several members do not agree on their preferences over some items so their inclusion in the group recommender system (GRS) may mislead the recommendation results. In this contribution a technique to detect and filter conflictive ratings before their use in the recommendation process is proposed and then its performance evaluated by using a well known recommendation dataset. The results show that rating filtering leads to improvements on GRSs performance.},
    author       = {Castro, Jorge and Barranco, Manuel J and Mart{ı}nez, Luis},
    booktitle    = {FLINS2014, Fuzzy Logic and Intelligent Technologies in Nuclear Science, Intelligent Systems and Knowledge Engineering},
    organization = {Da Ruan;Jie Lu},
    title        = {Improving group recommendation with outlier data filtering},
    year         = {2014},
}

