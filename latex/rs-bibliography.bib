    doi-num   = {10.1109/IMSAA.2008.4753931},
    issn      = {},
    keywords  = {fuzzy logic;genetic algorithms;mobile devices;mobile tourism multimedia selection;multimedia content;recommender system;fuzzy set theory;genetic algorithms;mobile handsets;multimedia communication;},
    month     = {dec.},
